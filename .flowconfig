[declarations]

[ignore]

[include]

[libs]
flow-typed

[lints]

[options]
; don't check all files
; all=true

; https://github.com/facebook/flow/issues/5180#issuecomment-366657780
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=./workspaces/frontend/src

module.file_ext=.js
module.file_ext=.css
module.file_ext=.scss
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
lazy_mode=fs

max_literal_length=200
[strict]
; sketchy-null
; sketchy-null-bool
; sketchy-null-string
; sketchy-null-number
; sketchy-null-mixed
sketchy-number
; sketchy-number-and
untyped-type-import
nonstrict-import
untyped-import
unclear-type
deprecated-type
deprecated-utility
; dynamic-export
unsafe-getters-setters
inexact-spread
unnecessary-optional-chain
unnecessary-invariant
signature-verification-failure
implicit-inexact-object
uninitialized-instance-property
non-array-spread

[version]
0.108.0
